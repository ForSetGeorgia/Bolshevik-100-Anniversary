@font-face {
    font-family: "caviar_dreams_r";
    src: url("<%= asset_path('fonts/caviar_dreams_r.ttf') %>") format("truetype");
}
@font-face {
    font-family: "caviar_dreams_b";
    src: url("<%= asset_path('fonts/caviar_dreams_b.ttf') %>") format("truetype");
}
@font-face {
    font-family: "playfair_display_r";
    src: url("<%= asset_path('fonts/playfair_display_r.otf') %>") format("opentype");
    font-style: normal;
    font-weight: 200;
}
@font-face {
    font-family: "playfair_display_b";
    src: url("<%= asset_path('fonts/playfair_display_b.otf') %>") format("opentype");
    font-style: normal;
    font-weight: 200;
}
